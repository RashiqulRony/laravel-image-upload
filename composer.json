{
    "name": "rashiqulrony/laravel-image-upload",
    "description": "Laravel image & file upload package with S3 support using Intervention Image.",
    "keywords": [
        "RashiqulRony",
        "Laravel Image Upload",
        "File Uploader",
        "Simple File and Image Uploader",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rashiqul Rony",
            "email": "rashiqulrony@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "intervention/image-laravel": "^1.5.6",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Rashiqulrony\\LaravelImageUpload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rashiqulrony\\LaravelImageUpload\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
