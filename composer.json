{
    "name": "rashiqulrony/laravel-image-upload",
    "description": "Laravel image & file upload package with s3 and using image intervention",
    "keywords": [
        "RashiqulRony",
        "Laravel Image Upload",
        "Media Uploader",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "rashiqulrony",
            "email": "rashiqulrony@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "intervention/image": "^3.11.2"
    },
    "autoload": {
        "psr-4": {
            "Rashiqulrony\\LaravelImageUpload\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers" : [
                "Rashiqulrony\\LaravelImageUpload\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
